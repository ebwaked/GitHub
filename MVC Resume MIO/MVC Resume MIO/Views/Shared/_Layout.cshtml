<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - Resume - MIO</title>

    <meta name="description" content="Resume Template">
    <meta name="author" content="kaylog">
    
    

    <!-- CSS -->
    <link href="/Content/css/bootstrap.min.css" rel="stylesheet" media="screen">
    <link href="/Content/css/style.css" rel="stylesheet">
    <link href="/Content/Site.css" rel="stylesheet" />
    <link href="/Content/font-awesome.min.css" rel="stylesheet" />
    <!-- More Css  -->

    <link href="/Content/css/bootstrap-theme.min.css" rel="stylesheet" />
    <link href="/Content/css/reveal.css" rel="stylesheet" />
    <link href="/Content/css/StyleSheet1.css" rel="stylesheet" />
    <link href="/Styles/shCoreMidnight.css" rel="stylesheet" />
    <link href="/Styles/shCore.css" rel="stylesheet" />
    <link href="/Styles/shThemeDefault.css" rel="stylesheet" />
    <link href="~/Content/BlogCSS/blog-home.css" rel="stylesheet">

    <!-- Font Awesome -->
    <link rel="stylesheet" href="/Content/font-awesome.min.css">


    <!-- Javascript not available -->
    <noscript>
        <link href="/Content/css/noscript.css" rel="stylesheet">
    </noscript>




    <![if lt IE 9]>
    <script src="/Scripts/html5shiv.js"></script>
    <![endif]
    <!-- To select the black text uncomment the following line -->
     <link href="/Content/css/dark.css" rel="stylesheet"> 
    <!-- To change the color of hover effects and links to green, you can uncomment the following line -->
     <!--<link href="~/Content/css/green.css" rel="stylesheet">-->
    <!-- To change the color of hover effects and links to orange, you can uncomment the following line -->
      <!--<link href="~/Content/css/orange.css" rel="stylesheet">-->

    <link href='http://fonts.googleapis.com/css?family=Droid+Sans:400,700' rel='stylesheet' type='text/css'>



    @Styles.Render("/Content/style.css")
    @Scripts.Render("/bundles/modernizr")
    
</head>
<body>

    @*<div id="blogfrontpage">
        @rendersection("blogfrontpage", required: false)
    </div>*@
    

    <div class="navbar navbar-inverse navbar-fixed-top">
        <div class="navbar-inner">
            <div class="container">
                <!-- This will display when the page is viewed on a mobile device -->
                <button type="button" class="btn btn-navbar" data-toggle="collapse">
                    <span class="icon-bar"></span>                          
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>      
                </button>
                @*<a class="brand me-name">Edward Waked</a> This text displays when the screen has been scrolled*@
                <div class="nav-collapse collapse pull-left navigation">
                    <ul class="nav">
                        <li class="active first">@Html.ActionLink("Home", "Index", "Home")</li>
                        <li>@Html.ActionLink("Keypoints", "Keypoints", "Home")</li>
                        <li>@Html.ActionLink("Resume", "Resume", "Home")</li>
                        <li>@Html.ActionLink("Experience", "Experience", "Home")</li>
                        <li>@Html.ActionLink("Portfolio", "Portfolio", "Home")</li>
                        <li>@Html.ActionLink("Education", "Education", "Home")</li>
                        <li>@Html.ActionLink("Skills", "Skills", "Home")</li>
                        <li>@Html.ActionLink("Blog", "Index", "Posts")</li>
                        <li>@Html.ActionLink("Contact", "Contact", "Home")</li>
                        <li>@Html.Partial("_LoginPartial")</li>
                    </ul>
                </div><!-- nav-collapse -->
            </div><!-- container -->
        </div><!-- navbar-inner -->
    </div><!-- navbar -->
    <div class="container">

        @RenderBody()
        @*@RenderBody("blogPost", false)*@
    </div>
    <div>
        <footer>
            <div class="container">
                <p>@DateTime.Now</p>
                <p>2015 Edward Waked - ASP.NET Resume</p>
            </div>
        </footer>
        <a href="#top" class="go-to-top">Top</a>
    </div>

    <!-- SCRIPTS -->
    <script src="/Scripts/jquery-2.1.3.min.js"></script>
    <script src="/Scripts/jquery.easing.min.js"></script>
    <script src="/Scripts/bootstrap.min.js"></script>
    <script src="/Scripts/scripts.js"></script>

    <!-- More Scripts -->
    <script src="/Scripts/Download.js"></script>
    <script src="/Scripts/JavaScript1.js"></script>
    <script src="/Scripts/jquery.dotdotdot.min.js"></script>

    @RenderSection("Scripts", required: false)
</body>
</html>
